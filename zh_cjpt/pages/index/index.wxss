.head>view {
  text-align: center;
  flex: 3;
  font-size: 30rpx;
  padding: 30rpx 0;
  font-family: "微软雅黑";
}
.tabbar view {
width: 50%;
height: 100%;
text-align: center;
font-size: 26rpx;
align-items: center;
flex: stretch;
display: flex;
flex-direction: column;
justify-content: center;

}
.tabbar view image {
width: 40rpx;
height: 45rpx;
display: block;
margin: 0rpx auto 0rpx;

}

button::after {
  content: " ";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: none;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}

.head {
  display: flex;
  border-bottom: 2rpx solid #e5e5e5;
  background: #fff;
}

.sele {
  color: red;
  border-bottom: 4rpx solid red;
}

.no_sele {
  color: blue;
}

.time {
  width: 40rpx;
  height: 40rpx;
  background: orange;
}

.list_0 {
  background: #fff;
  padding: 0 20rpx;
  margin-bottom: 40rpx;
}

.img_0 {
  width: 64rpx;
  height: 64rpx;
}

.img_1 {
  width: 50rpx;
  height: 60rpx;
}

.info_0 {
  padding: 20rpx 0;
}

.info_0>view text {
  font-size: 24rpx;
  padding: 0 10rpx;
  vertical-align: middle;
}

.info_0>view image {
  vertical-align: middle;
}

.info_0>view {
  display: inline-block;
  vertical-align: middle;
}

.info_0>view:nth-child(2) {
  float: right;
  line-height: 60rpx;
}

.info_1 {
  border-top: 2rpx solid #e5e5e5;
  padding: 20rpx 0;
}

.info_1>view {
