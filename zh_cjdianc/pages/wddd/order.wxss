.swiper-tab {
  background: #fff;
  position: fixed;
  padding: 0rpx 120rpx;
  top: 0;
  width: 100%;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  color: #ccc;
