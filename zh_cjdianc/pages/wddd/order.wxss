  margin: 0 10px;
}

.show {
  display: block;
}

.hidden {
  display: none;
}

.sdasd {
  display: flex;
}

button::after {
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
  border: none;
}
