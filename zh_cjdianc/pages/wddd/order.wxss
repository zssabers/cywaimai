@import "../../../wuBaseWxss/7.wxss";

page {
  height: 100%;
  font-family: '微软雅黑', Helvetica, sans-serif;
  background: #f8f8f8;
}

.navbar navigator {
  height: 100%;
  width: 1%;
}

.navbar navigator>view {
  width: 100%;
  padding-top: 4px;
}

.tab-bar {
  background: #fff;
  top: 90rpx;
  left: 0;
  width: 100%;
  z-index: 1000;
  position: fixed;
}

.tab-bar~.order-list {
  padding-top: 24rpx;
  padding-bottom: 115rpx;
  padding: 0 24rpx;
  margin-top: 0rpx;
}

.tab-bar navigator {
  display: block;
  padding: 0 24rpx;
  text-align: center;
}

.tab-bar navigator text {
  height: 90rpx;
  line-height: 90rpx;
  padding-bottom: 10rpx;
}

.tab-bar navigator.active text {
  color: #f00;

}

.order-list .order-item {
  background: #fff;
  padding: 32rpx 24rpx 0;
  margin-bottom: 20rpx;
  width: 702rpx;
  border-radius: 10rpx;

}

.order-list .order-info {
  font-size: 10pt;
  color: #888;
}

.goods-item {
  background: #fff;
  padding: 12rpx 0;
  margin: 0rpx 0 0rpx;
}

.goods-item:first-child {
  
}

.goods-item .goods-pic {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

.goods-item .goods-name {
  margin-bottom: 0rpx;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.goods-item .attr-list, .goods-item .num {
  font-size: 9pt;
  color: #888;
}

.goods-item .attr-list .attr-item {
  display: inline-block;
  margin-right: 36rpx;
}

.goods-item .attr-list .attr-item:last-child {
  margin-right: 0;
}

.goods-item .price {
  color: #888;
}

.order-option-btn {
  border: 1rpx solid #bbb;
  background: #fff;
  border-radius: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: inherit;
  margin-left: 30rpx;
  padding: 0 30rpx;
}

.order-option-btn::after {
  display: none;
}

.return-index-btn {
  position: fixed;
  right: 0;
  bottom: 100rpx;
  display: inline-block;
  background: rgba(157, 157, 157, 0.5);
  padding: 10rpx 20rpx;
  z-index: 1000;
  border-radius: 10rpx 0 0 10rpx;
}

.hide {
  display: none;
}

.modal1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}

