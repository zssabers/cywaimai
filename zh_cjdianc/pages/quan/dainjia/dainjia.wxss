}

.hyj {
  text-decoration: none;
  background: #fee890;
  color: #fa4118;
  padding: 5rpx 10rpx;
  border-radius: 5rpx;
}

.bg {
  background: #ff7d5c;
  position: relative;
  height: 110rpx;
}

.blbanner {
  padding: 10rpx 20rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.ddlb {
  display: flex;
  flex-direction: row;
  background: #fff;
  padding: 40rpx 20rpx;
}

.ddlb>view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.tjlb, .wmlb {
  border-top: 1rpx solid #f4f4f4;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  margin-bottom: 30rpx;
  background: #fff;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
}

.tjlb>view {
  background: #fff;
  flex: 1;
  padding: 0rpx;
  margin: 20rpx 0;
  display: flex;
  border-right: 1rpx solid #d9d9d9;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}

.image {
  width: 40rpx;
  height: 40rpx;
}

.num {
  font-size: 35rpx;
  color: #FF7D5C;
}
.nums{
