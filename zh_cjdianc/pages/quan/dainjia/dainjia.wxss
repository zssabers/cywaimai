  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 1rpx 16rpx 0rpx rgba(216, 216, 216, 0.5);
  border-radius: 10rpx;
  top: 20rpx;
  padding: 0rpx 20rpx;
}

.sjleftimg {
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
  border-radius: 10rpx;
}

.sjright1-l {
  width: 380rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.sc {
  font-size: 20rpx;
}

.sc image {
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}

.sjright2 {
  font-size: 20rpx;
}

.sjright2-l {
  min-width: 30rpx;
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}

.sjright1, .sjright2, .sjright3 {
  height: 50rpx;
}

.sjright3text {
  line-height: 30rpx;
  height: 30rpx;
  font-size: 24rpx;
  border-right: 1rpx solid #fff;
}

.sjright3-r {
  width: 13rpx;
