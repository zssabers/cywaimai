page {
  height: 100%;
  background: #fff;
}

.map {
  position: relative;
}

.heads {
  width: 100%;
  z-index: 100;
  padding: 15rpx 20rpx;
  display: flex;
  justify-content: space-between;
}

.foom {
  width: 594rpx;
}

.youbian {
  height: 60rpx;
  display: flex;
  align-items: center;
}

.youbian image {
  margin-right: 8rpx;
  width: 29rpx;
  height: 29rpx;
}

.label {
  height: 60rpx;
  display: flex;
  align-items: center;
  background-color: #f5f5f5;
  border-radius: 10rpx;
}

.shu {
  height: 60rpx;
  line-height: 60rpx;
}

.shuru image {
  margin-top: 10rpx;
  margin-right: 10rpx;
  width: 30rpx;
  height: 30rpx;
}

.shuru {
  display: flex;
  padding-left: 20rpx;
}

.label-i {
  display: flex;
  align-items: center;
  padding-right: 20rpx;
  padding-left: 20rpx;
}







.searcher input {
	padding: 6px 8px;
	background: white;
	font-size: 14px;
	color: #ccc;
	border-radius: 5px;
	text-align: center;
}

.placeholder-class {
	color: #ccc;
	text-align: center;
}

.header {
	background: #eee;
	font-size: 14px;
	padding: 10px;
	line-height: 24px;
	color: #444;


}

.current {
	color: #666666;
	padding: 10px;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.current text {
	font-size: 14px;
	width: 100%;
}

.current image {
	width: 30rpx;
	height: 30rpx;
  margin-right: 10rpx;
}


/*结果行*/
.item {
	padding: 10px;
	display: flex;
	flex-direction: column;
	border-bottom: 1px solid #eee;
}

.item .title {
	width: 100%;
	font-size: 18px;
	line-height: 30px;
	color: #333;
}

.item .address {
	width: 100%;
	font-size: 14px;
	line-height: 24px;
	color: #666;
}
.yinc{
  position: relative;
  top: -160rpx;
  background: #fff;
  height: 100%;
}
.bottom-bar {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 1000;
}

.bottom-bar navigator {
    background: #ff4544;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    color: #fff;
}

.address-list {
    padding-bottom: 110rpx;
}

.address-item {
    background: #fff;
    padding: 25rpx 30rpx;
    border-bottom: 1rpx solid #E3E3E3;
}

.userinfo {
    margin-bottom: 24rpx;
}

.address-detail {
}

.address-option {
    margin-left: 48rpx;
}

.address-option image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.btn {
    display: block;
    font-size: 11pt;
    color: #555;
    padding: 16rpx;
    line-height: normal;
    background: #fff;
    border-radius: 10rpx;
    text-align: center;
}

.btn:active {
    background: #fff;
    opacity: .65;
}

.btn:after {
    display: none;
}

.btn.btn-green {
    background: #09bb07;
    color: #fff;
    border-color: #09bb07;
}

.btn.btn-red {
    background: #ff4544;
    color: #fff;
    border-color: #ff4544;
}

.weui-cell__ft {
    display: flex;
    align-items: center;
}

.weui-cell__ft:nth-child(2) {
    margin-right: 30rpx;
}

.weui-cell__ft image {
    width: 45rpx;
    height: 45rpx;
    margin-right: 10rpx;
}

.radio-group .weui-cell {
    padding: 20rpx 30rpx;
    font-size: 28rpx;
    background: #fff;
}

.weui-panel__ft {
    margin-bottom: 25rpx;
}

.tjdz {
    position: fixed;
    width: 100%;
    bottom: 0;
    padding: 20rpx 0;
    background: #f4f4f4;
}

.jz {
    display: flex;
    align-items: center;
    justify-content: center;
}
.dingwe{
  display: flex;
  align-items: center;

}