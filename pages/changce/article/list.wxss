.fui-card {
  background: #fff;
  box-shadow: 0 0.1rem 0.1rem rgba(204, 204, 204, 0.3);
  border: 1px solid #ccc;
  margin: 0.5rem;
  position: relative;
  border-radius: 0.1rem;
  font-size: 30rpx;
  padding-top: 0.5rem;
  -moz-transition-duration: 300ms;
  transition-duration: 300ms;
  transition-property: background-color;
}

.fui-tab-scroll.fixed {
  position: fixed;
  top: 0rpx;
  z-index: 1000;
  text-align: center;
}

.fui-card.fui-card-full {
  margin: 0;
  border: none;
  border-radius: none;
  box-shadow: none;
}

.fui-card:not(.fui-card-list):active {
  background: #e5e5e5;
}

.fui-card .fui-card-title {
  padding: 0rem 0.5rem 0 0.5rem;
}

.fui-card .fui-card-title span {
  width: 100%;
  display: block;
}

.fui-card .fui-card-title .title {
  line-height: 1rem;
  font-size: 0.85rem;
}

.fui-card .fui-card-title .subtitle {
  color: #666;
  padding: 0.3rem 0;
}

.fui-card .fui-card-image {
  padding: 0.3rem 0.3rem 0 0.3rem;
}

.fui-card .fui-card-image img {
  width: 100%;
}

.fui-card .fui-card-content {
  padding: 0 0.5rem 0.5rem 0.5rem;
  line-height: 1rem;
  font-size: 0.7rem;
  color: #445;
}

.fui-card .fui-card-footer {
  border-top: 1px solid #f5f5f5;
  padding: 0.6rem 0.5rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.fui-card .fui-card-footer .text {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
}

.fui-card .fui-card-footer .remark:after {
  content: " ";
  display: inline-block;
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  height: 0.3rem;
  width: 0.3rem;
  border-width: 2px 2px 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  position: relative;
  top: -1px;
  margin-left: 0.3em;
}

.fui-card-header {
  text-align: center;
  margin: 0.8rem 0.5rem 0.8rem 0.5rem;
  font-size: 0.7rem;
  color: #666;
}

.fui-card-header span {
  display: inline-block;
  background: #cecece;
  padding: 0.2rem 0.5rem;
  border-radius: 0.2rem;
  color: #fff;
}

.fui-card.fui-card-list {
  padding: 0.05rem;
  padding-bottom: 0;
}

.fui-card .fui-card-first {
  padding: 0.5rem 0.2rem;
  -moz-transition-duration: 300ms;
  transition-duration: 300ms;
  transition-property: background-color;
}

.fui-card .fui-card-first .image {
  position: relative;
}

.fui-card .fui-card-first img {
  width: 100%;
  display: block;
}

.fui-card .fui-card-first .text {
  position: absolute;
  background: rgba(0, 0, 0, 0.8);
  bottom: 0;
  width: 97%;
  padding: 0.4rem 0.3rem;
  color: #fff;
  font-size: 32rpx;
}

.fui-card .fui-card-item {
  border-top: 1px solid #f5f5f5;
  padding: 0.2rem 0.5rem 0.2rem 0.5rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -moz-transition-duration: 300ms;
  transition-duration: 300ms;
  transition-property: background-color;
}

.fui-card .fui-card-item img {
  width: 2rem;
  height: 2rem;
  display: table-cell;
}

.fui-card .fui-card-item .text {
  padding: 0 0.5rem;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
}

.fui-card .fui-card-item .text.left {
  padding: 0;
}

.fui-card-first:active,
.fui-card-item:active {
  background: #e5e5e5;
}

.fui-card .fui-card-info {
  padding: 0 0.5rem;
  padding-top: 0.5rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.fui-card .fui-card-info span {
  display: block;
  padding: 0 0.5rem;
  font-size: 0.7rem;
}

.fui-card .fui-card-info .subtitle {
  padding-top: 0.2rem;
  color: #666;
  font-size: 0.65rem;
}

.fui-card .fui-card-info img {
  width: 2rem;
  height: 2rem;
}

.fui-card .fui-card-info .text {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
}

.fui-card .fui-card-btns {
  padding: 0.6rem 0.5rem;
  border-top: 1px solid #f5f5f5;
}

.fui-card .fui-card-btns a {
