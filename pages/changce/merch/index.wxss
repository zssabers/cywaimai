}
.searchcion {
  position: absolute;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}

.search_arr {
  background-color: #f7f7f7;
  border-radius: 30px;
  padding: 0rpx 20rpx;
  display: flex;
  width: 67%;
  height: 70rpx;
  position: relative;
}

.search_arr input {
  height: 70rpx;
  font-size: 27rpx;
  margin-left: 40rpx;
  width: 50%;
  border-radius: 5px;
}

.phcolor {
  color: #e2e2e2;
}

.classify {
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  width: 100%;
  top: -30rpx;
  height:100%;
  display: flex;
  position: relative;
  z-index: 99999999;


 
}
.nav_left {
  width: 25%;
  background: #fafafa;
  text-align: center;
  display: flex;
  flex-direction: column;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.nav_left .nav_left_items {
  font-size: 26rpx;
  color: rgba(0, 0, 0, 1);
  height:80rpx;
  line-height:80rpx;
  opacity: 0.6;
  background: #fafafa;
}

.nav_left .nav_left_items.active {
  background: white;
  color: #ff9b35;
  font-weight: 500;
  position: relative;
}

.nav_left .nav_left_items.active::before {
  display: inline-block;
  width: 4rpx;
  height: 40rpx;
  background: #ff9b35;
  content: '';
  position: absolute;
  left: 20rpx;
  top:20rpx;
}
.meiyou{
  top: 0;
  right: 0;
  height: 100%;
  padding: 0rpx 0 20rpx 36rpx;
  box-sizing: border-box;
  background: #fff;
}
.nav_right {
  top: 0;
  right: 0;
  width: 75%;
  height: 1000rpx;
  padding: 0rpx 0 20rpx 36rpx;
  padding-bottom: 40rpx;
  box-sizing: border-box;
  background: #fff;
}

.nav_right .nav_right_items {
  border-bottom: 4rpx solid #f2f2f2;

}

.hotel {
  display: flex;
  padding-top: 24rpx;
}

.hotelImg {
  width: 23%;
  border-radius: 10rpx;
  border:4rpx solid #f2f2f2;
}

.hotelImg image {
  width: 100%;
  height: 122rpx;
}

.hotelMain {
  width: 56%;
  padding: 20rpx;
}

.name {
  font-weight: bold;
  font-size: 28rpx;
}

.label {
  color: #cf9666;
  font-size: 22rpx;
  line-height: 50rpx;
}

.gobtn {
  width: 10%;
  color: #fff;
  border: 1rpx solid #ff9b35;
  background: #ff9b35;
  line-height: 50rpx;
  font-size: 22rpx;
  text-align: center;
  height: 50rpx;
  padding: 3rpx 8rpx;
  border-top-left-radius: 7rpx;
  border-bottom-left-radius: 7rpx;
  margin-top: 30rpx;
}

.invitepersonal {
  display: flex;
  padding: 20rpx 0rpx;
}

.inviteimage {
  width: 30%;
  padding: 0 8rpx;
  overflow: hidden;
}

.invitepersonal image {
  width: 100%;
}






